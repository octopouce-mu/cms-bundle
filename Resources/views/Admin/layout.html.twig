{% extends '@OctopouceAdmin/layout.html.twig' %}

{% trans_default_domain 'cms' %}

{% if form is defined %}
    {% form_theme form '@OctopouceAdmin/Form/layout.html.twig' %}
{% endif %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/octopoucecms/build/css/style.css') }}">
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/octopoucecms/build/js/cms.js') }}"></script>
{% endblock %}